<script>
	$(function() {
		$(".step").on("click", function() {
			var step = $(this).attr('value');
			console.log(step);
			var code = $('input[name=bct_code]').val();
			console.log(code);
			var url = "stepCal?emp_no=" + step+"&bct_code="+code;
			Aj(url, "#cal");
			function Aj(url, position) {
				$.ajax({
					url : url,
					type : "post",
					success : function(data) {
						$(position).html(data);
					},
					error : function(error) {
						console.log("error");
					}
				}); //ajax End
			}
		})
	});
</script>
